<div class="mx-auto max-w-2xl">
  <h1 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">My DevSpace</h1>
  <p class="mt-2 text-lg leading-8 text-gray-600">My web dev blog</p>
  <div class="mt-10 space-y-16 border-t border-gray-200 pt-10 sm:mt-16 sm:pt-16">
    <% @posts.each do |post| %>
      <%= render post %>
    <% end %>
  </div>
</div>
